<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:tag="layout/cell_exchange_middle_0"
    android:background="@drawable/cell_bg_middle"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginStart="16dp"
    android:layout_marginEnd="16dp">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/status_layout"
        android:tag="binding_1"
        android:background="@drawable/transaction_bg"
        android:layout_width="36dp"
        android:layout_height="36dp"
        android:layout_marginTop="16dp"
        android:layout_marginStart="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">
        <ImageView
            android:id="@+id/imageView3"
            android:tag="binding_2"
            android:layout_width="16dp"
            android:layout_height="16dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
        <com.google.android.material.imageview.ShapeableImageView
            android:id="@+id/image_from"
            android:tag="binding_3"
            android:layout_width="12dp"
            android:layout_height="12dp"
            android:scaleType="centerCrop"
            android:layout_weight="1"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:shapeAppearanceOverlay="@style/circleImageView"/>
        <TextView
            android:textSize="16sp"
            android:textColor="@color/white"
            android:id="@+id/textView54"
            android:tag="binding_4"
            android:visibility="gone"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:textAlignment="center"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            style="@style/Text.Inter.SemiBold"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <TextView
        android:textSize="13sp"
        android:textColor="@color/black"
        android:id="@+id/textView55"
        android:tag="binding_5"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="13dp"
        android:maxLines="1"
        android:singleLine="true"
        android:layout_marginStart="14dp"
        android:layout_marginEnd="16dp"
        android:paddingVertical="4dp"
        app:layout_constraintEnd_toStartOf="@+id/textView57"
        app:layout_constraintStart_toEndOf="@+id/status_layout"
        app:layout_constraintTop_toTopOf="parent"
        style="@style/Text.Inter.Medium"/>
    <TextView
        android:textSize="10sp"
        android:textColor="@color/middle_gray"
        android:id="@+id/textView56"
        android:tag="binding_6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="-2dp"
        android:paddingVertical="4dp"
        app:layout_constraintStart_toStartOf="@+id/textView55"
        app:layout_constraintTop_toBottomOf="@+id/textView55"
        style="@style/Text.Inter"/>
    <TextView
        android:textSize="15sp"
        android:textColor="@color/black"
        android:id="@+id/textView57"
        android:tag="binding_7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="21dp"
        android:paddingVertical="3dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView55"
        style="@style/Text.Inter.Medium"/>
    <View
        android:tag="binding_8"
        android:background="@color/black"
        android:layout_width="0dp"
        android:layout_height="1dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView57"
        app:layout_constraintEnd_toEndOf="@+id/textView57"
        app:layout_constraintStart_toStartOf="@+id/textView57"
        app:layout_constraintTop_toTopOf="@+id/textView57"/>
    <TextView
        android:textSize="10sp"
        android:textColor="@color/middle_gray"
        android:id="@+id/textView60"
        android:tag="binding_9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="-2dp"
        android:layout_marginEnd="21dp"
        android:paddingVertical="4dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView57"
        style="@style/Text.Inter"/>
</androidx.constraintlayout.widget.ConstraintLayout>
