                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginHorizontal="16dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tvDomiciliaryCompany">
                <RadioButton
                    android:id="@+id/rbYes"
                    android:background="@drawable/sl_text_input_bg"
                    android:layout_width="match_parent"
                    android:layout_height="60dp"
                    android:checked="false"
                    android:button="@null"
                    android:text="@string/yes"
                    android:drawableRight="@drawable/radiobutton_selector"
                    android:paddingStart="14dp"
                    android:paddingEnd="10dp"
                    style="@style/Text.Inter.Medium.14"/>
                <RadioButton
                    android:layout_gravity="start"
                    android:id="@+id/rbNo"
                    android:background="@drawable/sl_text_input_bg"
                    android:layout_width="match_parent"
                    android:layout_height="60dp"
                    android:layout_marginTop="11dp"
                    android:checked="false"
                    android:button="@null"
                    android:text="@string/no"
                    android:drawableRight="@drawable/radiobutton_selector"
                    android:paddingStart="14dp"
                    android:paddingEnd="10dp"
                    style="@style/Text.Inter.Medium.14"/>
            </RadioGroup>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.core.widget.NestedScrollView>
    <Button
        android:enabled="false"
        android:id="@+id/button_done"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="@dimen/bottom_margin"
        android:text="@string/continue_btn"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        style="@style/AuthButton"/>
    <Button
        android:enabled="false"
        android:id="@+id/button_done_k"
        android:background="@drawable/bg_button_green_wo_cornet"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:text="@string/continue_btn"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        style="@style/AuthButton"/>
</LinearLayout>
