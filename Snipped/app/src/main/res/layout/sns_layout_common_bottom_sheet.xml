<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <com.sumsub.sns.core.widget.SNSBottomSheetView
        android:orientation="vertical"
        android:id="@+id/sns_bottom_sheet"
        android:paddingBottom="@dimen/sns_margin_small"
        android:focusable="true"
        android:clickable="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingStart="@dimen/sns_margin_medium"
        android:paddingEnd="@dimen/sns_margin_medium"
        app:layout_behavior="@string/bottom_sheet_behavior">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/sns_content"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
    </com.sumsub.sns.core.widget.SNSBottomSheetView>
</merge>
