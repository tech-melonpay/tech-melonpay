<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:tag="layout/cell_input_text_0"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <com.luminary.business.presentation.ui.views.customtextinput.CustomTextInputLayout
        android:textColorHint="@color/middle_gray"
        android:id="@+id/inputLayout"
        android:tag="binding_1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="11dp"
        android:layout_marginBottom="0dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        app:hintTextAppearance="@style/TextHint"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        style="@style/Widget.Luminary.TextInputLayout">
        <com.luminary.business.presentation.ui.views.customtextinput.CustomTextInputEditText
            android:id="@+id/inputView"
            android:tag="binding_2"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            style="@style/Widget.Luminary.TextInputEditTextNoHint"/>
    </com.luminary.business.presentation.ui.views.customtextinput.CustomTextInputLayout>
    <ImageView
        android:id="@+id/imageView6"
        android:tag="binding_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/inputLayout"
        app:layout_constraintEnd_toEndOf="@+id/inputLayout"
        app:layout_constraintTop_toTopOf="@+id/inputLayout"
        app:srcCompat="@drawable/ic_arrow_down"/>
</androidx.constraintlayout.widget.ConstraintLayout>
