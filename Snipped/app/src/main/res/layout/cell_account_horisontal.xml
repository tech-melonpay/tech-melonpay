<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:tag="layout/cell_account_horisontal_0"
    android:background="@drawable/cell_bg_interactive_16"
    android:layout_width="wrap_content"
    android:layout_height="146dp"
    android:layout_marginTop="0dp"
    android:layout_marginBottom="0dp"
    android:minWidth="157dp"
    android:layout_marginStart="8dp"
    android:layout_marginEnd="0dp">
    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/imageView2"
        android:tag="binding_1"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginTop="18dp"
        android:scaleType="centerCrop"
        android:layout_marginStart="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:shapeAppearanceOverlay="@style/circleImageView"
        app:srcCompat="@drawable/ic_usd"/>
    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/icon_small"
        android:tag="binding_2"
        android:layout_width="14dp"
        android:layout_height="14dp"
        android:layout_marginBottom="-1dp"
        android:src="@drawable/ic_warning"
        android:scaleType="centerCrop"
        android:layout_marginEnd="-1dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2"
        app:layout_constraintEnd_toEndOf="@+id/imageView2"
        app:shapeAppearanceOverlay="@style/circleImageView"/>
    <TextView
        android:textSize="13sp"
        android:textColor="@color/red"
        android:gravity="center_vertical"
        android:id="@+id/tvFreeze"
        android:tag="binding_3"
        android:layout_width="wrap_content"
        android:layout_height="24dp"
        android:layout_marginTop="2dp"
        android:text="@string/wallet_is_temporarily_frozen"
        android:paddingEnd="8dp"
        android:layout_marginStart="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"/>
    <TextView
        android:textSize="18sp"
        android:textColor="@color/black"
        android:id="@+id/text1"
        android:tag="binding_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="26dp"
        android:lines="1"
        android:paddingEnd="16dp"
        android:layout_marginStart="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        style="@style/Text.Inter.Medium"/>
    <TextView
        android:textSize="13sp"
        android:textColor="@color/neutral_500"
        android:id="@+id/key1"
        android:tag="binding_5"
        android:layout_width="wrap_content"
        android:layout_height="24dp"
        android:layout_marginTop="2dp"
        android:maxLines="1"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text1"
        style="@style/Text.Inter.14"/>
    <TextView
        android:textSize="13sp"
        android:textColor="@color/red"
        android:gravity="center_vertical"
        android:id="@+id/tvFreezeLong"
        android:tag="binding_6"
        android:layout_width="wrap_content"
        android:layout_height="24dp"
        android:layout_marginTop="2dp"
        android:text="@string/wallet_is_temporarily_frozen"
        android:paddingEnd="8dp"
        android:layout_marginStart="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/key1"
        app:layout_constraintStart_toEndOf="@+id/key1"/>
</androidx.constraintlayout.widget.ConstraintLayout>
