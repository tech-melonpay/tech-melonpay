<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:tag="layout/dialog_sort_statistics_0"
    android:background="@color/transparent"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:background="@drawable/bg_sort_dialog"
        android:paddingBottom="44dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">
        <TextView
            android:textSize="18sp"
            android:textColor="@color/black"
            android:id="@+id/title_text"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:textAlignment="center"
            android:layout_marginStart="50dp"
            android:layout_marginEnd="50dp"
            app:layout_constraintBottom_toBottomOf="@+id/close"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/close"
            style="@style/Text.Inter.Medium"/>
        <ImageView
            android:id="@+id/close"
            android:background="@drawable/white_circle"
            android:padding="14dp"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginTop="18dp"
            android:layout_marginBottom="21dp"
            android:layout_marginEnd="16dp"
            app:layout_constraintBottom_toTopOf="@+id/customDate"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_close"/>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/customDate"
            android:background="@drawable/cell_bg_full_16"
            android:layout_width="0dp"
            android:layout_height="72dp"
            android:layout_marginTop="14dp"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/close">
            <ImageView
                android:id="@+id/calendar"
                android:background="@drawable/circle_selector_unchecked"
                android:padding="10dp"
                android:layout_width="36dp"
                android:layout_height="36dp"
                android:src="@drawable/ic_filter"
                android:layout_marginEnd="14dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>
            <TextView
                android:textSize="15sp"
                android:textColor="@color/black"
                android:gravity="start"
                android:id="@+id/customDateTitle"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="13dp"
                android:text="@string/from_date_to_date"
                android:layout_marginStart="16dp"
                android:paddingVertical="4dp"
                app:layout_constraintEnd_toStartOf="@+id/calendar"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                style="@style/Text.Inter.Medium"/>
            <TextView
                android:textSize="12sp"
                android:textColor="@color/middle_gray"
                android:id="@+id/customDateDescription"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="2dp"
                android:layout_marginBottom="13dp"
                android:text="@string/configure_custom_date"
                android:paddingVertical="3dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/calendar"
                app:layout_constraintStart_toStartOf="@+id/customDateTitle"
                app:layout_constraintTop_toBottomOf="@+id/customDateTitle"
                style="@style/Text.Inter"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/last7Days"
            android:background="@drawable/cell_bg_full_16"
            android:layout_width="0dp"
            android:layout_height="72dp"
            android:layout_marginTop="14dp"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/customDate">
            <CheckBox
                android:id="@+id/last7DaysCheckBox"
                android:background="@drawable/checkbox_selector"
                android:duplicateParentState="true"
                android:layout_width="36dp"
                android:layout_height="36dp"
                android:checked="false"
                android:button="@null"
                android:layout_marginEnd="18dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>
            <TextView
                android:textSize="15sp"
                android:textColor="@color/black"
                android:gravity="start"
                android:id="@+id/last7DaysTitle"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="13dp"
                android:text="@string/last_7_days"
                android:layout_marginStart="16dp"
                android:paddingVertical="4dp"
                app:layout_constraintEnd_toStartOf="@+id/last7DaysCheckBox"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                style="@style/Text.Inter.Medium"/>
            <TextView
                android:textSize="12sp"
                android:textColor="@color/middle_gray"
                android:id="@+id/last7DaysDescription"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="2dp"
                android:layout_marginBottom="13dp"
                android:text="@string/show_statistics_7_days"
                android:paddingVertical="3dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/last7DaysCheckBox"
                app:layout_constraintStart_toStartOf="@+id/last7DaysTitle"
                app:layout_constraintTop_toBottomOf="@+id/last7DaysTitle"
                style="@style/Text.Inter"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/last30Days"
            android:background="@drawable/cell_bg_full_16"
            android:layout_width="0dp"
            android:layout_height="72dp"
            android:layout_marginTop="14dp"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/last7Days">
            <CheckBox
                android:id="@+id/last30DaysCheckBox"
                android:background="@drawable/checkbox_selector"
                android:duplicateParentState="true"
                android:layout_width="36dp"
                android:layout_height="36dp"
                android:checked="false"
                android:button="@null"
                android:layout_marginEnd="18dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>
            <TextView
                android:textSize="15sp"
                android:textColor="@color/black"
                android:gravity="start"
                android:id="@+id/last30DaysTitle"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="13dp"
                android:text="@string/last_30_days"
                android:layout_marginStart="16dp"
                android:paddingVertical="4dp"
                app:layout_constraintEnd_toStartOf="@+id/last30DaysCheckBox"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                style="@style/Text.Inter.Medium"/>
            <TextView
                android:textSize="12sp"
                android:textColor="@color/middle_gray"
                android:id="@+id/last30DaysDescription"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="2dp"
                android:layout_marginBottom="13dp"
                android:text="@string/show_statistics_30_days"
                android:paddingVertical="3dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/last30DaysCheckBox"
                app:layout_constraintStart_toStartOf="@+id/last30DaysTitle"
                app:layout_constraintTop_toBottomOf="@+id/last30DaysTitle"
                style="@style/Text.Inter"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/last12Months"
            android:background="@drawable/cell_bg_full_16"
            android:layout_width="0dp"
            android:layout_height="72dp"
            android:layout_marginTop="14dp"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/last30Days">
            <CheckBox
                android:id="@+id/last12MonthsCheckBox"
                android:background="@drawable/checkbox_selector"
                android:duplicateParentState="true"
                android:layout_width="36dp"
                android:layout_height="36dp"
                android:checked="false"
                android:button="@null"
                android:layout_marginEnd="18dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>
            <TextView
                android:textSize="15sp"
                android:textColor="@color/black"
                android:gravity="start"
                android:id="@+id/last12MonthsTitle"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="13dp"
                android:text="@string/last_12_month"
                android:layout_marginStart="16dp"
                android:paddingVertical="4dp"
                app:layout_constraintEnd_toStartOf="@+id/last12MonthsCheckBox"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                style="@style/Text.Inter.Medium"/>
            <TextView
                android:textSize="12sp"
                android:textColor="@color/middle_gray"
                android:id="@+id/last12MonthsDescription"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="2dp"
                android:layout_marginBottom="13dp"
                android:text="@string/show_statistics_12_months"
                android:paddingVertical="3dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/last12MonthsCheckBox"
                app:layout_constraintStart_toStartOf="@+id/last12MonthsTitle"
                app:layout_constraintTop_toBottomOf="@+id/last12MonthsTitle"
                style="@style/Text.Inter"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/thisWeek"
            android:background="@drawable/cell_bg_full_16"
            android:layout_width="0dp"
            android:layout_height="72dp"
            android:layout_marginTop="14dp"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/last12Months">
            <CheckBox
                android:id="@+id/thisWeekCheckBox"
                android:background="@drawable/checkbox_selector"
                android:duplicateParentState="true"
                android:layout_width="36dp"
                android:layout_height="36dp"
                android:checked="false"
                android:button="@null"
                android:layout_marginEnd="18dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>
            <TextView
                android:textSize="15sp"
                android:textColor="@color/black"
                android:gravity="start"
                android:id="@+id/thisWeekTitle"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="13dp"
                android:text="@string/this_week"
                android:layout_marginStart="16dp"
                android:paddingVertical="4dp"
                app:layout_constraintEnd_toStartOf="@+id/thisWeekCheckBox"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                style="@style/Text.Inter.Medium"/>
            <TextView
                android:textSize="12sp"
                android:textColor="@color/middle_gray"
                android:id="@+id/thisWeekDescription"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="2dp"
                android:layout_marginBottom="13dp"
                android:text="@string/show_statistics_this_week"
                android:paddingVertical="3dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/thisWeekCheckBox"
                app:layout_constraintStart_toStartOf="@+id/thisWeekTitle"
                app:layout_constraintTop_toBottomOf="@+id/thisWeekTitle"
                style="@style/Text.Inter"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/thisMonth"
            android:background="@drawable/cell_bg_full_16"
            android:layout_width="0dp"
            android:layout_height="72dp"
            android:layout_marginTop="14dp"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/thisWeek">
            <CheckBox
                android:id="@+id/thisMonthCheckBox"
                android:background="@drawable/checkbox_selector"
                android:duplicateParentState="true"
                android:layout_width="36dp"
                android:layout_height="36dp"
                android:checked="false"
                android:button="@null"
                android:layout_marginEnd="18dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>
            <TextView
                android:textSize="15sp"
                android:textColor="@color/black"
                android:gravity="start"
                android:id="@+id/thisMonthTitle"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="13dp"
                android:text="@string/this_month"
                android:layout_marginStart="16dp"
                android:paddingVertical="4dp"
                app:layout_constraintEnd_toStartOf="@+id/thisMonthCheckBox"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                style="@style/Text.Inter.Medium"/>
            <TextView
                android:textSize="12sp"
                android:textColor="@color/middle_gray"
                android:id="@+id/thisMonthDescription"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="2dp"
                android:layout_marginBottom="13dp"
                android:text="@string/show_statistics_this_month"
                android:paddingVertical="3dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/thisMonthCheckBox"
                app:layout_constraintStart_toStartOf="@+id/thisMonthTitle"
                app:layout_constraintTop_toBottomOf="@+id/thisMonthTitle"
                style="@style/Text.Inter"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/thisYear"
            android:background="@drawable/cell_bg_full_16"
            android:layout_width="0dp"
            android:layout_height="72dp"
            android:layout_marginTop="14dp"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/thisMonth">
            <CheckBox
                android:id="@+id/thisYearCheckBox"
                android:background="@drawable/checkbox_selector"
                android:duplicateParentState="true"
                android:layout_width="36dp"
                android:layout_height="36dp"
                android:checked="false"
                android:button="@null"
                android:layout_marginEnd="18dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>
            <TextView
                android:textSize="15sp"
                android:textColor="@color/black"
                android:gravity="start"
                android:id="@+id/thisYearTitle"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="13dp"
                android:text="@string/this_year"
                android:layout_marginStart="16dp"
                android:paddingVertical="4dp"
                app:layout_constraintEnd_toStartOf="@+id/thisYearCheckBox"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                style="@style/Text.Inter.Medium"/>
            <TextView
                android:textSize="12sp"
                android:textColor="@color/middle_gray"
                android:id="@+id/thisYearDescription"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="2dp"
                android:layout_marginBottom="13dp"
                android:text="@string/show_statistics_this_year"
                android:paddingVertical="3dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/thisYearCheckBox"
                app:layout_constraintStart_toStartOf="@+id/thisYearTitle"
                app:layout_constraintTop_toBottomOf="@+id/thisYearTitle"
                style="@style/Text.Inter"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
